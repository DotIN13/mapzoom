{
  "configVersion": "v3",
  "app": {
    "appId": 1037690,
    "appName": "ZenMap",
    "appType": "app",
    "version": {
      "code": 1,
      "name": "1.0.1"
    },
    "icon": "icon.png",
    "vender": "DotIN13",
    "description": "ZenMap, an intuitive navigation tool designed to effortlessly guide your explorations."
  },
  "permissions": [
    "data:os.device.info",
    "device:os.geolocation",
    "device:os.local_storage",
    "device:os.compass"
  ],
  "runtime": {
    "apiVersion": {
      "compatible": "3.0.0",
      "target": "3.0.0",
      "minVersion": "3.0"
    },
    "type": 0
  },
  "debug": false,
  "targets": {
    "gt": {
      "module": {
        "page": {
          "pages": [
            "page/gt/home/index.page",
            "page/gt/map-transfer/index.page"
          ]
        },
        "app-side": {
          "path": "app-side/index"
        },
        "setting": {
          "path": "setting/index"
        }
      },
      "platforms": [
        {
          "st": "r",
          "dw": 480
        }
      ],
      "designWidth": 480
    }
  },
  "i18n": {
    "en-US": {
      "appName": "ZenMap"
    },
    "zh-CN": {
      "appName": "知莫地图"
    }
  },
  "defaultLanguage": "en-US"
}
